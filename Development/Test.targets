<Project>
  <PropertyGroup>
    <TestProjectType>UnitTest</TestProjectType>
  </PropertyGroup>
  <Import Project="$(SolutionDir)Development\Base.targets" />
  <PropertyGroup> 
    <TargetFrameworks>net48;net7.0</TargetFrameworks>
    <OutDir>$(BuildDir)\Regression\$(TargetFramework)</OutDir>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Coverage|x86' Or '$(Configuration)|$(Platform)' == 'Coverage|x64'">
    <DebugType>none</DebugType>
  </PropertyGroup>
  <PropertyGroup>
    <NoWarn>CA1707;CA5394</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="NUnit" Version="3.13.3"/>
    <PackageReference Include="NUnit.Analyzers" Version="3.8.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>