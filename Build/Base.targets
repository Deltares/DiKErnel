<Project>
  <PropertyGroup>
    <OutDir>$(OutputPath)</OutDir>
    <CodeAnalysisRuleSet>MinimumRecommendedRules.ruleset</CodeAnalysisRuleSet>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <FileAlignment>512</FileAlignment>
    <Company>Deltares</Company>
    <Copyright>Copyright Â© Stichting Deltares and State of the Netherlands 2023</Copyright>
    <Version>23.1.1</Version>
    <Configurations>Debug;Release;Coverage</Configurations>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|x86'">
    <ConfigurationGroup>Debug</ConfigurationGroup>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <DefineConstants>TRACE;DEBUG</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|x86'">
    <ConfigurationGroup>Release</ConfigurationGroup>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
    <DefineConstants>TRACE;RELEASE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Coverage|x86'">
    <ConfigurationGroup>Release</ConfigurationGroup>
    <DefineConstants>TRACE;COVERAGE</DefineConstants>
    <Optimize>true</Optimize>
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="$(SolutionDir)build\GlobalAssembly.cs">
      <Link>Properties\GlobalAssembly.cs</Link>
    </Compile>
  </ItemGroup>
</Project>